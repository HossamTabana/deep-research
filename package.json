{
  "name": "open-deep-research",
  "version": "1.0.0",
  "description": "An OpenSource Deep Research library with reasoning",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist/**"
  ],
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "pkgroll --minify --sourcemap=inline --src=./src",
    "test": "jest",
    "test:report": "tsx examples/basic/testFinalReport.ts",
    "dev:server": "tsx src/server.ts",
    "lint": "biome lint .",
    "prepare": "npm run build",
    "start": "node dist/index.js",
    "format": "biome check --write .",
    "dev": "tsx examples/basic/simple-research.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.2",
    "@ai-sdk/deepinfra": "^0.1.5",
    "@ai-sdk/google": "^1.1.2",
    "@ai-sdk/groq": "^1.1.2",
    "@ai-sdk/openai": "^1.1.9",
    "@types/uuid": "^10.0.0",
    "ai": "4.3.15",
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "jigsawstack": "^0.2.2",
    "json-schema": "0.4.0",
    "ts-retry": "^6.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.10",
    "dotenv": "^16.5.0",
    "pkgroll": "^2.6.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "ai",
    "research",
    "openai",
    "google-ai"
  ],
  "author": "",
  "license": "ISC"
}
